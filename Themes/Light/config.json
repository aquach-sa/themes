{
  "autoprefixer": {
    "browsers": [
      "last 2 versions",
      "safari 5",
      "ie 9",
      "opera 12.1",
      "ios 6",
      "android 4"
    ],
    "cascade": true
  },
  "app": "./Themes/Light/app.js",
  "css": "./Themes/Light/app.scss",
  "bower": [
    "./bower_components/jquery/dist/jquery.js",
    "./bower_components/angular/angular.js",
    "./bower_components/angular-ui-router/release/angular-ui-router.js",
    "./bower_components/bootstrap-sass/assets/javascripts/bootstrap.js",
    "./bower_components/bootstrap-datepicker/dist/js/bootstrap-datepicker.min.js",
    "./bower_components/ng-focus-if/focusIf.min.js",
    "./bower_components/angular-vertilize/angular-vertilize.min.js"
  ],
  "ng": [
    "./Themes/Light/app.js",
    "./Themes/Light/directives/**/*.js",
    "./Themes/Light/services/**/*.js",
    "./Themes/Light/views/**/*.js"
  ],
  "js": [
    "./Themes/Light/bower.js",
    "./Themes/Light/ng.js"
  ],
  "copy": [
    "./Themes/Light/**/*.html",
    "./Themes/Light/app.min.js",
    "./Themes/Light/theme.js",
    "./Themes/Light/MFAStyleSheet.css",
    "./Themes/Light/*.Master",
    "./Themes/Light/*.vb",
    "!./Themes/Light/app.js",
    "!./Themes/Light/app.scss",
    "!./Themes/Light/config.json"
  ],
  "build": [
    "./Themes/Light/app.min.js",
    "./Themes/Light/MFAStyleSheet.css"
  ],
  "dest": "./dist/Light",
  "source": "./Themes/Light",
  "pristine": [
    "./Themes/Light/app.min.js",
    "./Themes/Light/bower.js",
    "./Themes/Light/ng.js",
    "./Themes/Light/MFAStyleSheet.css"
  ],
  "watch": "./Themes/Light/**/*",
  "deploy": [],
  "url": "",
  "devDest": ""
}